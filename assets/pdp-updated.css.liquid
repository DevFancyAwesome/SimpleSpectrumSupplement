h4 {
  margin-top: 0;
  margin-bottom: 1.4vw;
  font-size: 5.4vw;
  font-weight: 800;
  line-height: 1.4;
}

.btn {
  display: block;
  height: 11.6vw;
  margin: 0 auto;
  padding: 0 8vw;
  border: none;
  outline: none;
  font-size: 3.6vw;
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 0.4vw;
  white-space: nowrap;
  cursor: pointer;
  transition: 0.2s;
}

.yellow-btn {
