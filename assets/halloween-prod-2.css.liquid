/***** # Product *****/
 .product {
	 text-align: center;
}
 .product .bold-ro__product {
	 display: flex;
	 flex-direction: column;
	 margin-top: 20px;
}
 .product .bold-ro__product .bold-ro__recurring-title {
	 display: none;
}
 .product .bold-ro__product .bold-ro__radio-div {
	 border: 2px solid #b1b1b1;
	 height: 6vw;
	 margin: 0;
	 font-size: 2vw;
	 letter-spacing: 0.1vw;
	 padding: 0 2vw;
	 text-align: left;
	 display: flex;
	 align-items: center;
}
 .product .bold-ro__product .bold-ro__radio-div.bold-ro__one-time-div {
	 border-bottom: none;
}
 .product .bold-ro__product .bold-ro__radio-div.bold-ro__bold-active {
	 background-color: #f9e7e4;
}
 .product .bold-ro__product .bold-ro__radio-div [type="radio"]:checked, .product .bold-ro__product .bold-ro__radio-div [type="radio"]:not(:checked) {
	 position: absolute;
	 left: -9999px;
}
 .product .bold-ro__product .bold-ro__radio-div [type="radio"]:checked + span, .product .bold-ro__product .bold-ro__radio-div [type="radio"]:not(:checked) + span {
	 position: relative;
	 padding-left: 40px;
	 cursor: pointer;
	 line-height: 20px;
	 display: inline-block;
	 color: #353336;
}
 .product .bold-ro__product .bold-ro__radio-div [type="radio"]:checked + span:before, .product .bold-ro__product .bold-ro__radio-div [type="radio"]:not(:checked) + span:before {
	 content: '';
	 position: absolute;
	 left: 0;
	 top: -3px;
	 width: 21px;
	 height: 21px;
	 border: 2px solid #c9523b;
	 border-radius: 100%;
	 background: #f5a494;
}
 .product .bold-ro__product .bold-ro__radio-div [type="radio"]:checked + span:after, .product .bold-ro__product .bold-ro__radio-div [type="radio"]:not(:checked) + span:after {
	 content: '\f00c';
	 color: #ca543e;
	 position: absolute;
	 top: -3px;
	 left: 5px;
	 font-size: 26px;
	 font-family: 'Font Awesome 5 Free';
	 font-weight: 600;
}
 .product .bold-ro__product .bold-ro__radio-div [type="radio"]:not(:checked) + span:after {
	 opacity: 0;
	 -webkit-transform: scale(0);
	 transform: scale(0);
}
 .product .bold-ro__product .bold-ro__radio-div [type="radio"]:checked + span:after {
	 opacity: 1;
	 -webkit-transform: scale(1);
	 transform: scale(1);
}
 .product img {
	 display: block;
	 width: 61.6vw;
	 margin: 0 auto 7vw;
}
 .product .selection {
	 position: relative;
	 display: flex;
	 justify-content: space-between;
	 margin-top: 6.4vw;
	 flex-direction: column;
}
 .product .selection .select {
	 order: 2;
}
 .product .selection .number {
	 order: 1;
	 margin-bottom: 20px;
	 text-align: left;
}
 .product .select ul {
	 display: none;
	 position: absolute;
	 bottom: 0;
	 left: 0;
	 transform: translateY(100%);
	 z-index: 1;
	 width: 100%;
}
 .product .select ul li, .product .select .active {
	 width: 100%;
	 padding: 0 2vw;
	 box-sizing: border-box;
	 border: 2px solid #e87a68;
	 border-top: none;
	 font-size: 2vw;
	 font-weight: 700;
	 text-align: left;
	 text-transform: uppercase;
	 line-height: 1.8;
	 letter-spacing: 0.23vw;
	 background-color: #fff;
	 cursor: pointer;
	 height: 6vw;
	 display: flex;
	 align-items: center;
}
 .product .select .active {
	 position: relative;
	 border-top: 2px solid #e87a68;
}
 .product .select .active:after {
	 content: "\f107";
	 position: absolute;
	 top: 50%;
	 right: 2vw;
	 transform: translateY(-50%);
	 width: 3vw;
	 height: 3.5vw;
	 font-family: 'Font Awesome 5 Free';
	 font-weight: 700;
	 font-size: 4vw;
	 line-height: 1;
}
 .product .selection input {
	 width: 33vw;
	 height: 12vw;
	 border: 2px solid #e87a68;
	 font-size: 3.4vw;
	 font-weight: 700;
	 text-align: center;
	 display: flex;
	 align-items: center;
}
 .product .yellow-btn {
	 width: 47.3vw;
	 margin-bottom: 8.7vw;
}
 .bold-ro__order-interval-container {
	 display: none;
}
